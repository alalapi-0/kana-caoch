/* 结果页样式：使用卡片展示测验与跟读信息 */
.container {
  padding: 48rpx;
  display: flex;
  flex-direction: column;
  gap: 24rpx;
}

.title {
  font-size: 36rpx;
  font-weight: bold;
}

.item {
  background: #ffffff;
  border-radius: 16rpx;
  padding: 24rpx;
  display: flex;
  flex-direction: column;
  gap: 8rpx;
}

.empty {
  color: #888;
}
